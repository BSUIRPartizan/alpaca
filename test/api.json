{
	"base": "http://localhost:3000",
	"version": "v1",
	"authorization": {
		"basic" : true,
		"header": true,
		"oauth" : true
	},
	"request": {
		"formats": {
			"json": true
		}
	},
	"response": {
		"formats": {
			"json": true
		},
		"suffix": false
	},
	"error": {
		"message": "error"
	},
	"class": {
		"client_options": {
			"base": {
				"path": "/"
			}
		},
		"methods": {
			"get": {
				"path": "/method"
			},
			"post": {
				"path": "/method",
				"method": "post"
			}
		}
	}
}
