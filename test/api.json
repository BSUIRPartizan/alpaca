{
	"base": "http://localhost:3000",
	"version": "v1",
	"authorization": {
		"basic" : true,
		"header": true,
		"oauth" : true
	},
	"request": {
		"formats": {
			"json": true
		}
	},
	"response": {
		"formats": {
			"default": "json",
			"json": true
		},
		"suffix": true
	},
	"error": {
		"message": "error"
	},
	"class": {
		"test": {
			"equal": {
				"path": "/test/equal"
			},
			"end": {
				"path": "/test/end"
			}
		},
		"client_options": {
			"base": {
				"path": "/index"
			}
		},
		"request_options": {
			"base": {
				"path": "/index"
			},
			"suffix": {
				"path": "/suffix"
			}
		},
		"get": {
			"api_params": {
				"path": "/get/api_params",
				"params": ["first", "second"]
			},
			"query_options": {
				"path": "/get/query_options"
			}
		},
		"response": {
			"basic": {
				"path": "/response/basic"
			},
			"header": {
				"path": "/response/header"
			},
			"html": {
				"path": "/response/html"
			},
			"json": {
				"path": "/response/json"
			}
		}
	}
}
