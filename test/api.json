{
	"base": "http://localhost:3000",
	"version": "v1",
	"authorization": {
		"basic" : true,
		"header": true,
		"oauth" : true
	},
	"request": {
		"formats": {
			"json": true
		}
	},
	"response": {
		"formats": {
			"json": true
		},
		"suffix": true
	},
	"error": {
		"message": "error"
	},
	"class": {
		"client_options": {
			"base": {
				"path": "/"
			}
		},
		"request_options": {
			"base": {
				"path": "/"
			},
			"suffix": {
				"path": "/request_options.html"
			},
			"response": {
				"path": "/request_options.json"
			}
		},
		"methods": {
			"get": {
				"path": "/method"
			},
			"post": {
				"path": "/method",
				"method": "post"
			},
			"put": {
				"path": "/method",
				"method": "put"
			},
			"patch": {
				"path": "/method",
				"method": "patch"
			},
			"delete": {
				"path": "/method",
				"method": "delete"
			}
		}
	}
}
